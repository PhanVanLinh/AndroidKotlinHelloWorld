<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >

    <Button
        android:id="@+id/button_test"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Test"
        />
    <Button
        android:id="@+id/button_test_higher_order_function"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Test Higher Order Function"
        />
    <Button
        android:id="@+id/button_instantiating_instance_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Instantiating a function type"
        />
    <Button
        android:id="@+id/button_invoke_function_type_instance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Invoking a function type instance"
        />
</LinearLayout>